{
  "name": "fuelr-clever-tanken",
  "description": "clever-tanken.de reverse engineered JSON API",
  "keywords": [
    "clever-tanken",
    "typescript",
    "api",
    "reverse-engineered"
  ],
  "version": "0.0.1",
  "main": "lib/index.js",
  "license": "MIT",
  "author": "vaaski <admin@vaa.ski>",
  "scripts": {
    "start": "node lib",
    "build": "rimraf lib && tsc",
    "prepare": "npm run build",
    "dev": "nodemon",
    "test": "nyc ava",
    "coverage": "live-server coverage/lcov-report",
    "prepublishOnly": "npm test",
    "commit": "cz -S"
  },
  "files": [
    "lib/**/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "tests/**/*"
    ],
    "require": [
      "ts-node/register"
    ],
    "failFast": true
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "ava": "^3.15.0",
    "commitizen": "^4.2.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ava": "^12.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "got": "^11.8.2"
  }
}
